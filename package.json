{
  "name": "annu-ng-lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build annu-ng-lib",
    "docs": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json",
    "docs:serve": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -s -w",
    "docs:json": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -e json -d dist/annu-ng-lib-pages/browser/data",
    "docs:json:serve": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -s -w -e json -d dist/annu-ng-lib-pages/browser/data",
    "dev:ssr": "ng run annu-ng-lib-pages:serve-ssr",
    "serve:ssr": "node dist/annu-ng-lib-pages/server/main.js",
    "build:ssr": "ng build --configuration production && ng run annu-ng-lib-pages:server:production && npm run docs:json",
    "prerender": "ng run annu-ng-lib-pages:prerender  && npm run docs:json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.3",
    "@angular/common": "~13.1.3",
    "@angular/compiler": "~13.1.3",
    "@angular/core": "~13.1.3",
    "@angular/forms": "~13.1.3",
    "@angular/platform-browser": "~13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.3",
    "@angular/platform-server": "~13.1.3",
    "@angular/router": "~13.1.3",
    "@nguniversal/express-engine": "^13.0.1",
    "express": "^4.15.2",
    "firebase": "^9.4.0",
    "firebaseui": "^6.0.0",
    "prismjs": "^1.26.0",
    "rxjs": "^7.5.4",
    "tslib": "^2.3.1",
    "ws": "^8.5.0",
    "xhr2": "^0.2.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.3",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@compodoc/compodoc": "^1.1.16",
    "@nguniversal/builders": "^13.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.1.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.4"
  }
}
