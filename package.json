{
  "name": "annu-ng-lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "build:lib": "ng build annu-ng-lib",
    "docs": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json",
    "docs:serve": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -s -w",
    "docs:json": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -e json -d dist/annu-ng-lib-pages/browser/data",
    "docs:json:serve": "npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -s -w -e json -d dist/annu-ng-lib-pages/browser/data",
    "dev:ssr": "ng run annu-ng-lib-pages:serve-ssr",
    "serve:ssr": "node dist/annu-ng-lib-pages/server/main.js",
    "build:ssr": "ng build annu-ng-lib-pages --configuration production && ng run annu-ng-lib-pages:server:production && npm run docs:json",
    "prerender": "ng run annu-ng-lib-pages:prerender  && npm run docs:json",
    "build:cordova": "ng build --configuration production --output-path=./cordova/www/ && npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -e json -d ./cordova/www/data",
    "build:cordova:ssr": "ng build --configuration production --output-path=./cordova/www/ && ng run annu-ng-lib-pages:server:production --output-path=./cordova/www/ && npx compodoc -p projects/annu-ng-lib/tsconfig.lib.json -e json -d ./cordova/www/data",
    "build:analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/annu-ng-lib-pages/browser/stats.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/platform-server": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@nguniversal/express-engine": "^15.0.0",
    "express": "^4.15.2",
    "firebase": "^9.15.0",
    "firebaseui": "^6.0.0",
    "prismjs": "^1.29.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nguniversal/builders": "^15.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "firebase-admin": "^11.4.1",
    "firebase-functions": "^4.1.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.0",
    "typescript": "~4.8.2"
  }
}
